package com.bolsadeideas.springboot.di.app.util.paginator;

public class PageRender {
	private int numero;
	private boolean actual;
	
	public PageRender(int numero, boolean actual) {
		this.numero = numero;
		this.actual = actual;
	}

	public int getNumero() {
		return numero;
	}

	public boolean isActual() {
		return actual;
	}
	
}
